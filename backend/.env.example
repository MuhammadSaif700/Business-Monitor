# Backend Configuration
BACKEND_PASSWORD=YOUR_SECURE_PASSWORD_HERE
BACKEND_API_KEY=YOUR_SECURE_API_KEY_HERE

# AI Configuration
AI_ENABLED=true

# OpenAI API (Optional - for AI features)
OPENAI_API_KEY=

# Hugging Face API (Optional - for AI features)
HUGGINGFACE_API_KEY=
HUGGINGFACE_MODEL=gpt2

# Google AI API (Recommended - for AI features)
GOOGLE_API_KEY=YOUR_GOOGLE_API_KEY_HERE
GOOGLE_MODEL=models/gemini-2.0-flash

# Database Configuration (SQLite by default)
DATABASE=business.db

# CORS Settings (comma-separated origins)
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000

# Production Settings
ENVIRONMENT=production
DEBUG=false
